{
    "name": "contact",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.4.0",
        "@testing-library/user-event": "^7.2.0",
        "@types/node": "^12.12.19",
        "@types/react": "^16.9.16",
        "@types/react-dom": "^16.9.4",
        "@types/react-router": "^5.1.3",
        "@types/react-router-dom": "^5.1.3",
        "node-sass": "^4.13.0",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-hook-form": "^3.29.4",
        "react-router": "^5.1.2",
        "react-router-dom": "^5.1.2",
        "react-scripts": "3.3.0",
        "typescript": "^3.7.3"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --watchAll=false --passWithNoTests --env=jsdom",
        "eject": "react-scripts eject",
        "storybook": "start-storybook -p 6006 -c .storybook",
        "build-storybook": "build-storybook -s public",
        "lint": "tslint -c tslint.json src/**/*.{ts,tsx} --format verbose",
        "format": "prettier --write README.md src/**/*.{ts,tsx,html,scss,css}"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint",
            "pre-push": "react-scripts test --watchAll=false --passWithNoTests"
        }
    },
    "lint-staged": {
        "*.{tsx,scss,html}": [
            "npm run lint"
        ],
        "*.ts": [
            "npm run lint",
            "git add"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@storybook/react": "^5.2.8",
        "@types/enzyme": "^3.10.4",
        "@types/enzyme-adapter-react-16": "^1.0.5",
        "@types/jest": "^24.0.24",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.15.1",
        "husky": "^3.1.0",
        "jest": "^24.9.0",
        "lint-staged": "^9.5.0",
        "ts-jest": "^24.2.0",
        "tslint": "^5.20.1",
        "tslint-config-prettier": "^1.18.0",
        "tslint-config-standard": "^9.0.0",
        "tslint-react": "^4.1.0"
    }
}
